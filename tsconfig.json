{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/liquid/*": ["./components/libs/*"],
      "@/liquid-layouts/*": ["./layouts/lib/*"],
      "@/liquid-forms/*": ["./packages/form/src/*"],
      "@/liquid-table/*": ["../liquid-next/packages/table/src/*"],
      "@liquid101/util/*": ["./utils/libs/*"],
      "@/liquid-store/*": ["../liquid-next/store/libs/*"],
      "@/liquid-domain/*": ["../liquid-next/domain/*"],
      "@/liquid-styles/*": ["../liquid-next/styles/*"],
      "@/test/*": ["../liquid-next/__tests__/*"],


    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", ],
  "exclude": [
    "node_modules",
    "**/setup.ts"
],}
